<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.keyframeanimationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.keyframeanimation.KeyFrameAnimationActivity"
            android:theme="@style/AppTheme.NoActionBar"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts"/>

        <activity
            android:name=".activity.keyframeanimation.ShoppingKeyFrameAnimation"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".activity.collpasingtoolbar.CollapsingToolbarActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".activity.parallax.ParallaxEffectActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".activity.circularpositioning.CircularPositioning"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>